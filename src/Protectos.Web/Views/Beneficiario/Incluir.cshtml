@model Protectos.Application.ViewModels.Beneficiarios.BeneficiarioViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutContent.cshtml";
}

@section header
{
    <section class="content-header">
        <h1>
            Index administradora
            <small>Administradora configurada</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>
            <li class="active">Here</li>
        </ol>
    </section>
}



    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-md-12">
                <!-- Custom Tabs -->
                <div class="nav-tabs-custom">
                    <ul class="nav nav-tabs">
                        <li class="active"><a href="#tab_1" data-toggle="tab">Dados</a></li>
                        <li><a href="#tab_2" data-toggle="tab">Produtos</a></li>
                        <li><a href="#tab_3" data-toggle="tab">Contatos</a></li>
                        <li><a href="#tab_4" data-toggle="tab">Cobrança</a></li>
                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                Dropdown <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Action</a></li>
                                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Another action</a></li>
                                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Something else here</a></li>
                                <li role="presentation" class="divider"></li>
                                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Separated link</a></li>
                            </ul>
                        </li>
                        <li class="pull-right"><a href="#" class="text-muted"><i class="fa fa-gear"></i></a></li>
                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane active" id="tab_1">
                            <!-- DADOS PESSOAIS -->
                            <div class="box box-primary">
                                <div class="box-header with-border">
                                    <h3 class="box-title">Dados cadastrais do beneficiario</h3>
                                </div>
                                <form role="form">
                                    <div class="box-body">                                    
                                        @Html.Partial("_DadosPessoais")
                                    </div>
                                </form>
                            </div>

                            <div class="btn-group btn-group-toggle" data-toggle="buttons">

                                <a class="btn btn-app">
                                    <i class="fa fa-edit"></i> Editar
                                </a>
                                <a class="btn btn-app">
                                    <i class="fa fa-save"></i> Salvar
                                </a>
                            </div>

                        </div>
                        <!-- /.tab-pane -->
                        <div class="tab-pane" id="tab_2">
                            <!--PRODUTOS -->
                            <div class="row">
                                <!--row 1-->
                                <div class="col-md-12">
                                    <div class="box">
                                        <div class="box-header with-border">
                                            <h3 class="box-title">Produtos</h3>
                                        </div>
                                        <!-- /.box-header -->
                                        <div class="box-body">
                                            <table class="table table-bordered">
                                                <tbody>
                                                    <tr>
                                                        <th style="width: 10px">#</th>
                                                        <th>Task</th>
                                                        <th>Progress</th>
                                                        <th style="width: 40px">Label</th>
                                                    </tr>
                                                    <tr>
                                                        <td>1.</td>
                                                        <td>Update software</td>
                                                        <td>
                                                            <div class="progress progress-xs">
                                                                <div class="progress-bar progress-bar-danger" style="width: 55%"></div>
                                                            </div>
                                                        </td>
                                                        <td><span class="badge bg-red">55%</span></td>
                                                    </tr>
                                                    <tr>
                                                        <td>2.</td>
                                                        <td>Clean database</td>
                                                        <td>
                                                            <div class="progress progress-xs">
                                                                <div class="progress-bar progress-bar-yellow" style="width: 70%"></div>
                                                            </div>
                                                        </td>
                                                        <td><span class="badge bg-yellow">70%</span></td>
                                                    </tr>
                                                    <tr>
                                                        <td>3.</td>
                                                        <td>Cron job running</td>
                                                        <td>
                                                            <div class="progress progress-xs progress-striped active">
                                                                <div class="progress-bar progress-bar-primary" style="width: 30%"></div>
                                                            </div>
                                                        </td>
                                                        <td><span class="badge bg-light-blue">30%</span></td>
                                                    </tr>
                                                    <tr>
                                                        <td>4.</td>
                                                        <td>Fix and squish bugs</td>
                                                        <td>
                                                            <div class="progress progress-xs progress-striped active">
                                                                <div class="progress-bar progress-bar-success" style="width: 90%"></div>
                                                            </div>
                                                        </td>
                                                        <td><span class="badge bg-green">90%</span></td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <!-- /.box-body -->
                                        <div class="box-footer clearfix">
                                            <ul class="pagination pagination-sm no-margin pull-right">
                                                <li><a href="#">«</a></li>
                                                <li><a href="#">1</a></li>
                                                <li><a href="#">2</a></li>
                                                <li><a href="#">3</a></li>
                                                <li><a href="#">»</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                    <!-- /.box -->


                                </div>
                            </div> <!--//row 1-->

                        </div>
                        <!-- /.tab-pane -->
                        <div class="tab-pane" id="tab_3">
                            Lorem Ipsum is simply dummy text of the printing and typesetting industry.
                            Lorem Ipsum has been the industry's standard dummy text ever since the 1500s,
                            when an unknown printer took a galley of type and scrambled it to make a type specimen book.
                            It has survived not only five centuries, but also the leap into electronic typesetting,
                            remaining essentially unchanged. It was popularised in the 1960s with the release of Letraset
                            sheets containing Lorem Ipsum passages, and more recently with desktop publishing software
                            like Aldus PageMaker including versions of Lorem Ipsum.
                        </div>
                        <div class="tab-pane" id="tab_4">
                            <!--DADOS DE COBRANCA-->
                            <div class="box box-primary">
                                <div class="box-header with-border">
                                    <h3 class="box-title">Dados de cobrança do beneficiario</h3>
                                </div>
                                <!-- form start -->
                                <form role="form">
                                    <div class="box-body">
                                        <div class="row">
                                            <!--row 1-->
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label>Forma de Pagamento:</label>
                                                    <select class="form-control text-uppercase" name="FormaPagamento">
                                                        <option>Boleto Bancario</option>
                                                        <option>Débito em Conta</option>

                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label>Banco:</label>
                                                    <select class="form-control text-uppercase" name="Banco">
                                                        <option value="1">Banco do Brasil</option>
                                                        <option selected="selected" value="2">Caixa Economica</option>
                                                        <option value="3">Bradesco</option>
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label>Data Nascimento:</label>
                                                    <div class="input-group">
                                                        <div class="input-group-addon">
                                                            <i class="fa fa-calendar"></i>
                                                        </div>
                                                        <input type="text" class="form-control" data-inputmask="'alias': 'dd/mm/yyyy'" data-mask="">
                                                    </div>
                                                    <!-- /.input group -->
                                                </div>
                                            </div>
                                        </div><!--//row-->
                                    </div>
                                </form>
                            </div>
                        </div>
                        <!-- /.tab-pane -->
                    </div>
                    <!-- /.tab-content -->
                </div>
                <!-- nav-tabs-custom -->
            </div>

        </div>



    </section>
    <!-- /.content -->
